[
  {
    "caption": "Preferences",
    "id": "preferences",
    "mnemonic": "n",
    "children": [
      {
        "caption": "Package Settings",
        "id": "package-settings",
        "mnemonic": "P",
        "children": [
          {
            "caption": "R-Factor",
            "id": "r-factor-settings",
            "children": [
              {
                "caption": "Settings – Default",
                "command": "open_file",
                "args": {
                  "file": "${packages}/r-factor/r-factor.sublime-settings"
                }
              },
              {
                "caption": "Settings – User",
                "command": "open_file",
                "args": {
                  "file": "${packages}/User/r-factor.sublime-settings"
                }
              },
              { "caption": "-" },
              {
                "caption": "Read Me",
                "command": "open_file",
                "args": {
                  "file": "${packages}/r-factor/README.md"
                }
              },
              {
                "caption": "License",
                "command": "open_file",
                "args": {
                  "file": "${packages}/r-factor/LICENSE"
                }
              },
              { "caption": "-" },
              {
                "caption": "Enter License",
                "command": "enter_license"
              }
            ]
          }
        ]
      }
    ]
  }
]
